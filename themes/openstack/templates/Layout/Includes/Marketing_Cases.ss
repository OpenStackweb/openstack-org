<h2>Case Studies</h2>
<hr>
<div class="item-list">
	<% loop LatestCases %>
	<div class="item span-7">
		<div class="span-2">
			$Preview
		</div>
		<div class="span-5 item-content last">
			<a rel="nofollow" href="$Link">$Name</a>
			<p>$Tagline</p>
		</div>
	</div>
	<% end_loop %>
	<a href="/user-stories">More >></a>
</div>